@model List<indiedevlog.web.EFModel.Objects.PlanUpdates>

@{
    ViewData["Title"] = "Home";
}

<h1>latest updates</h1>

@foreach (var post in @Model)
{
    <div class="postContainer">
        <h3>@post.Subject</h3>
        
        <div class="content">
            <p>@post.Body</p>
            <div class="footer">
                <span class="user">@post.UserID</span>
                <span class="date">@post.Modified.ToString("MM-dd-yyyy")</span>
            </div>
        </div>
    </div>
}