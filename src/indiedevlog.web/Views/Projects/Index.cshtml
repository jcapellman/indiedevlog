@model indiedevlog.web.Models.ProjectModel

@{
    ViewData["Title"] = "Projects";
}

<h1>Projects</h1>

<div class="registerContainer">
    Enter your plan update below<br /><br />

    <form asp-action="CreateProject" method="post">
        <input asp-for="Name" class="form-control" required placeholder="subject" /><br />
        
        <input type="submit" value="submit" class="btn btn-default" />
    </form>
</div>


@foreach (var project in @Model.ProjecListing)
{
    <div>
        @project.Name
    </div>
}